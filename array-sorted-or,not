#include <stdio.h>

int main() {
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr) / sizeof(arr[0]);
    int sorted = 1;

    for (int i = 0; i < n - 1; i++) {
        if (arr[i] > arr[i + 1]) {
            sorted = 0; 
            break;
        }
    }

    if (sorted)
        printf("true\n");
    else
        printf("false\n");

    return 0;
}
