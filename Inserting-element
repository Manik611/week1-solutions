#include <stdio.h>

int main() {
    int arr[100] = {10, 20, 30, 40}; 
    int n = 4;  
    int ele;

    printf("Enter element to insert at end: ");
    scanf("%d", &ele);

    arr[n] = ele; 
    n++; 
    printf("Array after insertion: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
