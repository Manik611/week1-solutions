#include <stdio.h>
#include <string.h>

int main() {
    char s[] = "abba";
    int n = strlen(s);
    int isPalindrome = 1; 

    for (int i = 0; i < n / 2; i++) {
        if (s[i] != s[n - i - 1]) {
            isPalindrome = 0; 
            break;
        }
    }

    printf("%d\n", isPalindrome);
    return 0;
}
